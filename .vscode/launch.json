{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "extendscript-debug",
			"request": "launch",
			"name": "Current script",
			"targetSpecifier": "framemaker-15.0",
			"engineName": "main",
			"program": "${file}",
			"excludes": [
				"builtin",
				"prototype",
			],
			"stopOnEntry": false,
		},
		{
			"type": "extendscript-debug",
			"request": "launch",
			"name": "View All Data",
			"targetSpecifier": "framemaker-15.0",
			"engineName": "main",
			"program": "${file}",
			"excludes": [],
			"stopOnEntry": false,
		},
		{
			"type": "extendscript-debug",
			"request": "launch",
			"name": "Exclude All Data",
			"targetSpecifier": "framemaker-15.0",
			"engineName": "main",
			"program": "${file}",
			"excludes": [
				"builtin",
				"Function",
				"prototype",
				"undefined",
			],
			"stopOnEntry": false,
		},
	]
}